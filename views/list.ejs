<%- include("header") -%>

 <div class="box" id="heading">
    <h1><%= kindOfList %> To Do</h1>
 </div>
    
<div class="box"><h3>
    <% for (var i=0;i<newListItem.length;i++){ %>
        <div class="item">
            <input type="checkbox"> 
                <p><%= newListItem[i] %></p>
        </div>
    <% } %></h3>
   
    <form method="post" action="/" class="item">
        <input type="text" name="newItem" placehoder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%= kindOfList %>>+</button>
    </form>
</div>   


<%- include("footer") -%>

